{
  "family": "dreampath-backend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::479324456849:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "479324456849.dkr.ecr.ap-northeast-2.amazonaws.com/dreampath-backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "PYTHON_AI_SERVICE_URL",
          "value": "https://ai.dreampathai.link"
        },
        {
          "name": "APP_FRONTENDURL",
          "value": "https://dreampathai.link"
        }
      ],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/database:DB_HOST::"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/database:DB_PORT::"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/database:DB_NAME::"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/database:DB_USERNAME::"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/database:DB_PASSWORD::"
        },
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/openai:OPENAI_API_KEY::"
        },
        {
          "name": "OPENAI_MODEL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/openai:OPENAI_MODEL::"
        },
        {
          "name": "MAIL_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/mail:MAIL_USERNAME::"
        },
        {
          "name": "MAIL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/mail:MAIL_PASSWORD::"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:GOOGLE_CLIENT_ID::"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:GOOGLE_CLIENT_SECRET::"
        },
        {
          "name": "KAKAO_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:KAKAO_CLIENT_ID::"
        },
        {
          "name": "KAKAO_REDIRECT_URI",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:KAKAO_REDIRECT_URI::"
        },
        {
          "name": "NAVER_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:NAVER_CLIENT_ID::"
        },
        {
          "name": "NAVER_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/oauth:NAVER_CLIENT_SECRET::"
        },
        {
          "name": "LIVEKIT_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/livekit:LIVEKIT_API_KEY::"
        },
        {
          "name": "LIVEKIT_API_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/livekit:LIVEKIT_API_SECRET::"
        },
        {
          "name": "LIVEKIT_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/livekit:LIVEKIT_URL::"
        },
        {
          "name": "PINECONE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_API_KEY::"
        },
        {
          "name": "PINECONE_INDEX_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_INDEX_NAME::"
        },
        {
          "name": "PINECONE_INDEX_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_INDEX_HOST::"
        },
        {
          "name": "PINECONE_FAQ_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_FAQ_API_KEY::"
        },
        {
          "name": "PINECONE_FAQ_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_FAQ_HOST::"
        },
        {
          "name": "PINECONE_FAQ_INDEX",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/pinecone:PINECONE_FAQ_INDEX::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:479324456849:secret:dreampath/jwt:JWT_SECRET::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dreampath-backend",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "wget -q --spider http://localhost:8080/actuator/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 120
      }
    }
  ]
}
